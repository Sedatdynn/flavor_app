{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch prod",// env name during run it
            "request": "launch",
            "type": "dart",
            "program": "lib/main_prod.dart",// file's path
            "args": ["--flavor", "prod", "--target", "lib/main_prod.dart"],
        },
        {
            "name": "Launch dev",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_dev.dart",
            "args": ["--flavor", "dev", "--target", "lib/main_dev.dart"],

        },
        {
            "name": "Launch staging",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_staging.dart",
            "args": ["--flavor", "staging", "--target", "lib/main_staging.dart"],

        }
    ]
}